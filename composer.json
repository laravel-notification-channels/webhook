{
    "name": "laravel-notification-channels/webhook",
    "description": "Webhook Notifications driver",
    "homepage": "https://github.com/laravel-notification-channels/webhook",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "m.pociot@gmail.com",
            "homepage": "http://www.marcelpociot.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/notifications": "~5.5 || ~6.0",
        "illuminate/support": "~5.5 || ~6.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "~7.0 || ~8.0",
        "orchestra/testbench": "^3.5.0 || ^4.0",
        "orchestra/database": "^3.5.0 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Webhook\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Webhook\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
