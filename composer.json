{
    "name": "laravel-notification-channels/webhook",
    "description": "Webhook Notifications driver",
    "homepage": "https://github.com/laravel-notification-channels/webhook",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "m.pociot@gmail.com",
            "homepage": "http://www.marcelpociot.com"
        },
        {
            "name": "atymic",
            "email": "atymicq@gmail.com",
            "homepage": "https://atymic.dev"
        }
    ],
    "require": {
        "php": "^7.2.5 || ^8.0",
        "guzzlehttp/guzzle": "^6.2 || ^7.0",
        "illuminate/notifications": "^6.0 || ^7.0 || ^8.0 || ^9.0",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0 || ^9.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^4.0 || ^5.0 || ^6.0 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Webhook\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Webhook\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:ci": "phpunit --coverage-clover=coverage.xml"
    },
    "config": {
        "sort-packages": true
    }
}
